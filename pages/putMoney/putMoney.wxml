    <view style="font-family:{{fontFamily}};font-size:{{fontSize}};">{{text}}</view>
  </view>


  <canvas canvas-id='cat' id='cat' style="width:100vw;border-bottom:1rpx solid #333;"></canvas>
  <canvas canvas-id='cat2' id='cat2' style="width:100vw;border-top:1rpx solid #333;border-bottom:1rpx solid #333;" disable-scroll="true" bindtouchstart="canvasStart" bindtouchmove="canvasMove" bindtouchend="canvasEnd" touchcancel="canvasEnd" binderror="canvasIdErrorCallback"></canvas>


  <canvas class="canvas" id="canvas" canvas-id="canvas" disable-scroll="true" bindtouchstart="canvasStart" bindtouchmove="canvasMove" bindtouchend="canvasEnd" touchcancel="canvasEnd" binderror="canvasIdErrorCallback"></canvas>
  <view bindtap="cleardraw" style="font-size:24rpx;text-align:center;padding:20rpx;background:#666;color:#fff;width:50rpx;">清除</view>
  <button type="default" size='mini' bindtap="savePic">导出图片</button>


  <view>
    <image src="{{imageUrl}}" style='width:100vw;height:100vh;'></image>
    <!-- <image src="data:image/png;base64,"
      style='width:100vw;height:100vh;'></image> -->
  </view>


  <button type="default" bindtap="base64ImageShow">Base64图片显示</button>

</view>