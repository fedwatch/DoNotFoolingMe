  <button open-type="openSetting" bindOpenSetting="getAuthority" size="mini">打开授权设置页</button>
  <view class="section">
    <!-- <view class="section__title">movable-view区域小于movable-area</view> -->

    <movable-area style="height: 400rpx; width: 100vw;" scale-area>
        <movable-view style="height: 100rpx; width: 100rpx; background:url('../../files/svg/023-gift.svg'); transform: rotate(33deg);"  direction="all" bindchange="onChange" bindscale="onScale" scale scale-min="0.3" scale-max="5" scale-value="1">
        </movable-view>
      </movable-area>

    <movable-area style="height: 400rpx; width: 100vw;">
    <movable-view style="height: 400rpx; width: 100vw;">
      <movable-area style="height: 100rpx; width: 100rpx; transform: rotate(33deg);">
        <movable-view style="height: 100rpx; width: 100rpx; background:url('../../files/svg/023-gift.svg');" x="{{x}}" y="{{y}}" direction="all">
        </movable-view>
      </movable-area>
         </movable-view>
         
    </movable-area>

    
    <view class="btn-area">
      <button size="mini" bindtap="tap">点击移动</button>
      <button size="mini" bindtap="addRotate">旋转</button>
    </view>
    <!--<view class="section__title">movable-view区域大于movable-area</view>-->
